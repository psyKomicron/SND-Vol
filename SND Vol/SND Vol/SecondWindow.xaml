            <local:AudioSessionView MinWidth="85" MaxHeight="90" HorizontalAlignment="Center"/>
            <local:AudioSessionView MinWidth="85" MaxHeight="90" HorizontalAlignment="Center"/>
        </GridView>
    </Grid>
</Window>
